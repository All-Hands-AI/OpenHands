# OpenHands Frontend Environment Configuration Template
# Copy this to .env and customize as needed
# WebSocket and API configuration for runtime environment

# Backend configuration
VITE_BACKEND_HOST=0.0.0.0:12000
VITE_BACKEND_BASE_URL=0.0.0.0:12000

# Frontend configuration
VITE_FRONTEND_PORT=12001

# TLS/SSL configuration
VITE_USE_TLS=false
VITE_INSECURE_SKIP_VERIFY=true

# Development mode
NODE_ENV=development