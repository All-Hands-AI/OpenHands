# Default configuration template for OpenDevin

# Core Configuration
# Uncomment and configure the following settings as needed

# [core]
# runtime = "server"
# file_store = "memory"
# file_store_path = "/tmp/file_store"
# workspace_base = "/path/to/workspace"
# workspace_mount_path = "/path/to/mount"
# workspace_mount_path_in_sandbox = "/workspace"
# workspace_mount_rewrite = "your_workspace_mount_rewrite"
# cache_dir = "/tmp/cache"
# sandbox_container_image = "ghcr.io/opendevin/sandbox:main"
# run_as_devin = true
# max_iterations = 100
# max_budget_per_task = 100.0
# e2b_api_key = "your_e2b_api_key"
# sandbox_type = "ssh"  # Options: "ssh", "exec", "e2b", "local"
# use_host_network = false
# ssh_hostname = "localhost"
# disable_color = false
# sandbox_user_id = 1000
# sandbox_timeout = 120
# debug = false

# Sandbox configuration
# Uncomment and configure the following settings as needed

# [sandbox]
# dockerfile = "Dockerfile"
# template_name = "open-devin"
# template_id = "785n69crgahmz0lkdw9h"

# LLM Providers Configuration
# Uncomment and configure the following settings as needed

# [llm]
# model = "gpt-4o"
# api_key = "your_api_key"
# base_url = "your_base_url"
# api_version = "your_api_version"
# embedding_model = "local"
# embedding_base_url = "your_embedding_base_url"
# embedding_deployment_name = "your_embedding_deployment_name"
# aws_access_key_id = "your_aws_access_key_id"
# aws_secret_access_key = "your_aws_secret_access_key"
# aws_region_name = "your_aws_region_name"
# num_retries = 5
# retry_min_wait = 3
# retry_max_wait = 60
# timeout = 30
# max_chars = 5000000
# temperature = 0.0
# top_p = 0.5
# custom_llm_provider = "your_custom_llm_provider"
# max_input_tokens = 2048
# max_output_tokens = 2048
# input_cost_per_token = 0.0001
# output_cost_per_token = 0.0001

# Agent Configuration
# Uncomment and configure the following settings as needed

# [agent]
# name = "CodeActAgent"
# memory_enabled = false
# memory_max_threads = 2

# Environment Variables
# Uncomment and configure the following settings as needed

# [env]
# VAR_NAME = "value"

# Example:
# [env]
# OPENAI_API_KEY = "your_openai_api_key"
# AZURE_SUBSCRIPTION_KEY = "your_azure_subscription_key"